<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
	<h4 class="modal-title" id="myModalLabel">{{ entity.name }}</h4>
</div>
<div class="modal-body">
	<table class="table table-striped">
		<tbody>
			<tr>
				<th>#</th>
				<td>{{ entity.id }}</td>
			</tr>
            <tr>
                <th>Name</th>
                <td><a href="{{ entity}}" target="_blank">{{ entity.name}}</a></td>
            </tr>
            <tr>
                <th>Domain</th>
                <td>{{ entity.domain}}</td>
            </tr>
            <tr>
                <th>Path</th>
				<td>{{ entity.path }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ entity.user }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.password }}</td>
            </tr>
            <tr>
                <th>Port</th>
                <td>{{ entity.port }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.Type }}</td>
            </tr>
            <tr>
                <th>Subtype</th>
                <td>{{ entity.subtype }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Latency</th>
                <td>{{ entity.latency }}</td>
            </tr>
            <tr>
                <th>Last Online</th>
                <td>{{ entity.lastOnline|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Last Check</th>
                <td>{{ entity.lastCheck|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Send email</th>
                <td>{{ entity.sendEmail }}</td>
            </tr>
            <tr>
                <th>Monitoring</th>
                <td>{{ entity.monitoring }}</td>
            </tr>
            <tr>
                <th>Createdby</th>
                <td>{{ entity.createdBy }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Modifiedby</th>
                <td>{{ entity.modifiedBy }}</td>
            </tr>
            <tr>
                <th>Modifieddate</th>
                <td>{{ entity.modifiedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>
</div>
<div class="modal-footer">
	<a href="{{ path('server_remove', { 'id': entity.id }) }}" class="btn btn-danger pull-left" data-toggle="modal" data-target="#delete_{{ entity.id }}">Delete</a>
	<button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
	<a href="{{ path('server_edit', { 'id': entity.id }) }}" class="btn btn-warning" data-toggle="modal" data-target="#edit_{{ entity.id }}">Edit</a>
</div>
